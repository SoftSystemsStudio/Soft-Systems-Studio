{
  "name": "apps-agent-api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "node dist/index.js",
    "build": "tsc -p tsconfig.json && cp -R prisma ./dist/prisma",
    "worker": "ts-node --transpile-only src/worker/ingestWorker.ts",
    "metrics": "ts-node --transpile-only src/metrics.ts",
    "prisma:generate": "prisma generate",
    "migrate:dev": "prisma migrate dev --name init",
    "migrate:deploy": "prisma migrate deploy",
    "seed": "ts-node prisma/seed.ts",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "test:integration": "NODE_ENV=test jest tests/integration --runInBand"
  },
  "bin": {},
  "dependencies": {
    "@prisma/client": "^5.8.0",
    "@softsystems/agent-customer-service": "workspace:^",
    "@softsystems/core-llm": "workspace:^",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "bullmq": "^5.65.0",
    "cookie-parser": "^1.4.7",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.0",
    "node-fetch": "2",
    "pino": "^8.16.0",
    "pino-http": "^11.0.0",
    "pino-pretty": "^13.1.2",
    "prom-client": "^14.0.1",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.10",
    "@types/jest": "^29.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node-fetch": "2",
    "@types/supertest": "^2.0.12",
    "eslint-plugin-security": "^3.0.1",
    "jest": "^29.0.0",
    "prisma": "^5.8.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.2"
  }
}
